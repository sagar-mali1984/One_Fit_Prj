.dashboard-stat{
    height: 90px;
    cursor: pointer;
}
.dashboard-stat .visual {
    width: 80px;
    height: 80px;
    display: block;
    float: left;
    padding-top: 10px;
    padding-left: 15px;
    margin-bottom: 10px;
}

.dashboard-stat .details {
    position: absolute;
    right: 15px;
    padding-right: 10px;
    margin-top: 10%;
    margin-bottom: 10%;
    
}

.dashboard-stat .details .desc {
    text-align: right;
    font-size: 18px;
    letter-spacing: 0px;
    font-weight: 500;
    color: #fff;
    word-wrap: break-word;
}

.dashboard-stat .visual i {
    font-size: 60px;
    line-height: 65px;
    color: #fff;
}

.dashboard-stat:before, .dashboard-stat:after {
    display: table;
    line-height: 0;
    content: "";
}

