}

.header .navbar-nav {
    margin-right: 20px;
    display: block;
}
.header .hor-menu ul.nav li {
    position: relative;
}
.header .navbar-nav > li {
    margin: 0px;
    padding: 0px;
}
.header .hor-menu ul.nav li a {
    font-size: 15px;
    padding: 11px 18px;
    font-weight: bold;
}
.header .navbar-nav > li.dropdown:last-child {
    padding-right: 2px;
}
.header .navbar-nav .dropdown-menu {
    margin-top: 3px;
}
.header .navbar-nav > li.dropdown, .header .navbar-nav > li.dropdown > a {
    padding-left: 4px;
    padding-right: 4px;
}
.nav.pull-right > li > .dropdown-menu, .nav > li > .dropdown-menu.pull-right {
    right: -10px;
    left: auto;
}
 .nav > li > .dropdown-menu:after {
    position: absolute;
    top: -6px;
    right: 10px;
    display: inline-block !important;
    border-right: 6px solid transparent;
    border-bottom: 6px solid #fff;
    border-left: 6px solid transparent;
    content: '' !important;
}

.header .navbar-nav > li.dropdown.user .dropdown-toggle {
    padding: 7px 4px 6px 9px;
}
.header .navbar-nav > li.dropdown > a:last-child {
    padding-right: 0;
}

.header .navbar-nav > li.dropdown.user .dropdown-toggle {
    padding: 7px 4px 6px 9px;
}

.header .navbar-nav .dropdown-toggle:hover, .header .navbar-nav .dropdown.open .dropdown-toggle {
    background-color: white !important;
}

.dropdown-menu>li>a:hover {
    color: #262626;
    text-decoration: none;
    background-color: #f5f5f5;
    cursor: pointer;
}


.fontcolor1 {
    color: #2b328e !important;
}
.dropdown-menu>li>a {
    display: block;
    padding: 3px 20px;
    clear: both;
    font-weight: 400;
    line-height: 1.42857143;
    color: #333;
    white-space: nowrap;
}

.navbar-nav>li>a {
    text-decoration: none;
}

.dropdown-toggle:after{
content:unset;
}

