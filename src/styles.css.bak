/* Fieldset css */

fieldset>.left{
    display: block; 
    float: left; 
    margin-top: -17px; 
    background-color: #FFF; 
    padding: 0 5px;
}

fieldset>.right{
    display: block; 
    float: right; 
    margin-top: -17px; 
    background-color: #FFF; 
    padding: 0 5px;
}

fieldset {
    position: relative;
}

.legend2 {
right: 1%;
line-height:1.2em;
border: 0;
background: #fff;
width: auto;
transform: translateY(-50%);
position: absolute;
top: 0;
padding: 0 .5em;
font-weight: bold;
font-size: 1rem;
color: #4472C4;
text-decoration: underline;
}

.legend2 a {
    cursor: pointer;
}

.customLegend {
border: 2px solid #70ad47;
padding: 1.5em 0px 0.5em;
margin-top: 2em;
position: relative;
}

.customLegend legend {
border: 0;
background: #fff;
width: auto;
transform: translateY(-50%);
position: absolute;
top: 0; left: 1%;
padding: 0 .5em;
font-weight: bold;
font-size: 1rem;
color: #7F7F7F;
}


/* Table css */

.TFtable{
    width:100%; 
}
.TFtable td{ 
    padding:7px;
}

.TFtable tr{
    background: #b8d1f3;
}

.TFtable th{
    background: #4472c4;
    color: white;
    padding:7px;
}
/*  Define the background color for all the ODD background rows  */
.TFtable tr:nth-child(odd){ 
    background: #CFD5EA;
}
/*  Define the background color for all the EVEN background rows  */
.TFtable tr:nth-child(even){
    background: #E9EBF5;
}

table {
    border-collapse: unset;
}


/* MenuBar and SubMenuBar css */

.nav-pills a{
    color: black;
}

.nav-pills .nav-link.active, .nav-pills .show>.nav-link {
    color: #fff;
    background-color: #4472c4;
    font-weight: bold;
}

.nav-pills li:nth-child(odd){
    background: #CFD5EA;
} 

.nav-pills li:nth-child(even){
    background: #E9EBF5;
} 

#tab .tab-content{
    width:86%;
}

.visible-xs
{
    display: none !important;
}

@media (min-width: 300px) and (max-width: 900px) {
    #tab .tab-content{
        width:100%;
    }
    .hidden-xs
    {
        display: none !important;
    }
    .visible-xs
    {
        display: block !important;
    }
    #AccountTabs > .nav{

        display: none;
      }
}

@media (min-width: 768px) and (max-width: 800px) {
    #tab .tab-content {
        width: 78%;
    }
    .hidden-xs
    {
        display: none !important;
    }
    .visible-xs
    {
        display: block !important;
    }
    #AccountTabs > .nav{

        display: none;
      }
}

@media (min-width: 813px) and (max-width: 1024px) {
    #tab .tab-content {
        width: 78%;
    }
    .hidden-xs
    {
        display: block !important;
    }
    .visible-xs
    {
        display: none !important;
    }
}

@media (min-width: 300px) and (max-width: 750px) {
    .customLegend {
        margin-top: 2em !important;
    }
    .hidden-xs
    {
        display: none !important;
    }
    .visible-xs
    {
        display: block !important;
    }
    #AccountTabs > .nav{

        display: none;
      }
}


